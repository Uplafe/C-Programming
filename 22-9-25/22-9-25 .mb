#include <stdio.h>

void main()
{int num1,num2;
num1= 10;
num2= 15;
printf("%d added by %d is %d",num1,num2,num1+num2);
printf("\n%d multipled by %d is %d",num1,num2,num1*num2);
printf("\n%d Subtracted by %d is %d",num1,num2,num1-num2);
printf("\n%d Subtracted by %d is %d",num2,num1,num2-num1);
printf("\n%d divided by %d is %d",num2,num1,num2/num1);
}
